[application]

config/name="Star Shooters"
config/description="A top-down 2D bullet-hell game with a focus on grazing bullets to gain power!"
run/main_scene="res://Menus/Main Menu.tscn"
boot_splash/image="res://BorisProductions.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.12549, 0.145098, 0.192157, 1 )
config/icon="res://Space Shooter Redux/PNG/playerShip1_blue.png"
config/windows_native_icon="res://DesktopIcons/Windows.ico"

[autoload]

SceneTransition="*res://Levels/SceneTransition.tscn"
BGM="*res://Levels/BGM.tscn"
GameManager="*res://GameManager.tscn"
UpgradeManager="*res://Player/UpgradeManager.tscn"
StatsManager="*res://Player/StatsManager.tscn"
GlobalWorldEnvironment="*res://WorldEnvironment.tscn"

[display]

mouse_cursor/custom_image="res://Space Shooter Redux/PNG/UI/cursor.png"
window/stretch/mode="2d"
window/stretch/aspect="keep"

